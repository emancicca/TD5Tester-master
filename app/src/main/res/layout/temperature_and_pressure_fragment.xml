<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:CircleProgressView="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mooo.hairyone.td5tester.MainActivity"
    android:padding="1sp"
    android:orientation="horizontal"
    android:baselineAligned="false">
    <LinearLayout
        android:layout_width="0sp"
        android:layout_height="match_parent"
        android:padding="1sp"
        android:orientation="vertical"
        android:layout_weight="1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0sp"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            tools:ignore="NestedWeights">
            <at.grabner.circleprogress.CircleProgressView
                android:id="@+id/gCOOLANT_TEMP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="10sp"
                CircleProgressView:cpv_autoTextSize="true"
                CircleProgressView:cpv_value="60"
                CircleProgressView:cpv_maxValue="120"
                CircleProgressView:cpv_showUnit="true"
                CircleProgressView:cpv_unit="CT °C"
                CircleProgressView:cpv_unitPosition="bottom"
                CircleProgressView:cpv_textMode="Value"
                CircleProgressView:cpv_barColor="@color/color1"
                CircleProgressView:cpv_barColor1="@color/color2"
                CircleProgressView:cpv_barColor2="@color/color3"
                CircleProgressView:cpv_barColor3="@color/color4"
                CircleProgressView:cpv_barWidth="10dp"
                CircleProgressView:cpv_fillColor="#293133"
                CircleProgressView:cpv_innerContourSize="0dp"
                CircleProgressView:cpv_blockCount="9"
                CircleProgressView:cpv_outerContourSize="0dp"
                CircleProgressView:cpv_rimWidth="0dp"
                CircleProgressView:cpv_startAngle="0"
                CircleProgressView:cpv_textColor="#ffffff"
                CircleProgressView:cpv_unitColor="#ffffff"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0sp"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">
            <at.grabner.circleprogress.CircleProgressView
                android:id="@+id/gINLET_TEMP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="10sp"
                CircleProgressView:cpv_autoTextSize="true"
                CircleProgressView:cpv_value="60"
                CircleProgressView:cpv_maxValue="120"
                CircleProgressView:cpv_showUnit="true"
                CircleProgressView:cpv_unit="IAT °C"
                CircleProgressView:cpv_unitPosition="bottom"
                CircleProgressView:cpv_textMode="Value"
                CircleProgressView:cpv_barColor="@color/color1"
                CircleProgressView:cpv_barColor1="@color/color2"
                CircleProgressView:cpv_barColor2="@color/color3"
                CircleProgressView:cpv_barColor3="@color/color4"
                CircleProgressView:cpv_barWidth="10dp"
                CircleProgressView:cpv_fillColor="#293133"
                CircleProgressView:cpv_innerContourSize="0dp"
                CircleProgressView:cpv_blockCount="9"
                CircleProgressView:cpv_outerContourSize="0dp"
                CircleProgressView:cpv_rimWidth="0dp"
                CircleProgressView:cpv_startAngle="0"
                CircleProgressView:cpv_textColor="#ffffff"
                CircleProgressView:cpv_unitColor="#ffffff"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0sp"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">
            <at.grabner.circleprogress.CircleProgressView
                android:id="@+id/gFUEL_TEMP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="10sp"
                CircleProgressView:cpv_autoTextSize="true"
                CircleProgressView:cpv_value="60"
                CircleProgressView:cpv_maxValue="120"
                CircleProgressView:cpv_showUnit="true"
                CircleProgressView:cpv_unit="FUEL °C"
                CircleProgressView:cpv_unitPosition="bottom"
                CircleProgressView:cpv_textMode="Value"
                CircleProgressView:cpv_barColor="@color/color1"
                CircleProgressView:cpv_barColor1="@color/color2"
                CircleProgressView:cpv_barColor2="@color/color3"
                CircleProgressView:cpv_barColor3="@color/color4"
                CircleProgressView:cpv_barWidth="10dp"
                CircleProgressView:cpv_fillColor="#293133"
                CircleProgressView:cpv_innerContourSize="0dp"
                CircleProgressView:cpv_blockCount="9"
                CircleProgressView:cpv_outerContourSize="0dp"
                CircleProgressView:cpv_rimWidth="0dp"
                CircleProgressView:cpv_startAngle="0"
                CircleProgressView:cpv_textColor="#ffffff"
                CircleProgressView:cpv_unitColor="#ffffff"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="0sp"
        android:layout_height="match_parent"
        android:padding="1sp"
        android:orientation="vertical"
        android:layout_weight="1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0sp"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            tools:ignore="NestedWeights">
            <at.grabner.circleprogress.CircleProgressView
                android:id="@+id/gAMBIENT_PRESSURE"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="10sp"
                CircleProgressView:cpv_autoTextSize="true"
                CircleProgressView:cpv_value="60"
                CircleProgressView:cpv_maxValue="120"
                CircleProgressView:cpv_showUnit="true"
                CircleProgressView:cpv_unit="AAP kPa"
                CircleProgressView:cpv_unitPosition="bottom"
                CircleProgressView:cpv_textMode="Value"
                CircleProgressView:cpv_barColor="@color/color1"
                CircleProgressView:cpv_barColor1="@color/color2"
                CircleProgressView:cpv_barColor2="@color/color3"
                CircleProgressView:cpv_barColor3="@color/color4"
                CircleProgressView:cpv_barWidth="10dp"
                CircleProgressView:cpv_fillColor="#293133"
                CircleProgressView:cpv_innerContourSize="0dp"
                CircleProgressView:cpv_blockCount="9"
                CircleProgressView:cpv_outerContourSize="0dp"
                CircleProgressView:cpv_rimWidth="0dp"
                CircleProgressView:cpv_startAngle="0"
                CircleProgressView:cpv_textColor="#ffffff"
                CircleProgressView:cpv_unitColor="#ffffff" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0sp"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">
            <at.grabner.circleprogress.CircleProgressView
                android:id="@+id/gMANIFOLD_AIR_PRESSURE"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                CircleProgressView:cpv_decimalFormat="#0.00"
                android:padding="10sp"
                CircleProgressView:cpv_autoTextSize="true"
                CircleProgressView:cpv_value="1.2"
                CircleProgressView:cpv_maxValue="1.50"
                CircleProgressView:cpv_showUnit="true"
                CircleProgressView:cpv_unit="MAP Bar"
                CircleProgressView:cpv_unitPosition="bottom"
                CircleProgressView:cpv_textMode="Value"
                CircleProgressView:cpv_barColor="@color/color1"
                CircleProgressView:cpv_barColor1="@color/color2"
                CircleProgressView:cpv_barColor2="@color/color3"
                CircleProgressView:cpv_barColor3="@color/color4"
                CircleProgressView:cpv_barWidth="10dp"
                CircleProgressView:cpv_fillColor="#293133"
                CircleProgressView:cpv_innerContourSize="0dp"
                CircleProgressView:cpv_blockCount="9"
                CircleProgressView:cpv_outerContourSize="0dp"
                CircleProgressView:cpv_rimWidth="0dp"
                CircleProgressView:cpv_startAngle="0"
                CircleProgressView:cpv_textColor="#ffffff"
                CircleProgressView:cpv_unitColor="#ffffff"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0sp"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">
            <at.grabner.circleprogress.CircleProgressView
                android:id="@+id/gAIR_FLOW"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="10sp"
                CircleProgressView:cpv_autoTextSize="true"
                CircleProgressView:cpv_value="350"
                CircleProgressView:cpv_maxValue="700"
                CircleProgressView:cpv_showUnit="true"
                CircleProgressView:cpv_unit="MAF kg/h"
                CircleProgressView:cpv_unitPosition="bottom"
                CircleProgressView:cpv_textMode="Value"
                CircleProgressView:cpv_barColor="@color/color1"
                CircleProgressView:cpv_barColor1="@color/color2"
                CircleProgressView:cpv_barColor2="@color/color3"
                CircleProgressView:cpv_barColor3="@color/color4"
                CircleProgressView:cpv_barWidth="10dp"
                CircleProgressView:cpv_fillColor="#293133"
                CircleProgressView:cpv_innerContourSize="0dp"
                CircleProgressView:cpv_blockCount="9"
                CircleProgressView:cpv_outerContourSize="0dp"
                CircleProgressView:cpv_rimWidth="0dp"
                CircleProgressView:cpv_startAngle="0"
                CircleProgressView:cpv_textColor="#ffffff"
                CircleProgressView:cpv_unitColor="#ffffff"/>
        </LinearLayout>
    </LinearLayout>


</LinearLayout>
